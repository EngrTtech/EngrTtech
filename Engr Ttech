function mmyownApproach(arr,sum){
  if(arr.length<4){
     return num ==arr.reduce((a,
 b)=>(a+b));
  }
  
  for(let i= 0;i< arr.length;i++){
  
    for(let j=i+1;j<arr.length;j++){
      let remainingElement = sum -
 (arr[i]+arr[j]);
        let remainingArray = arr.slice(j+1,
 arr.length);
 
        if(remainingArray.
 includes(remainingElement)){
         return true;
       }
      }
     
    }
    
 return false;
 
 }
